soap.wsdl_cache_dir     = "{{ php_config_main_module_soap_wsdl_cache_dir }}"
soap.wsdl_cache_ttl     = {{ php_config_main_module_soap_wsdl_cache_ttl }}
soap.wsdl_cache_limit   = {{ php_config_main_module_soap_wsdl_cache_limit }}


[sysvshm]
sysvshm.init_mem = {{ php_config_main_module_sysvshm_init_mem }}


[ldap]
ldap.max_links   = {{ php_config_main_module_ldap_max_links }}


[mcrypt]
mcrypt.algorithms_dir   = {{ php_config_main_module_mcrypt_algorithms_dir }}
mcrypt.modes_dir        = {{ php_config_main_module_mcrypt_modes_dir }}


[dba]
dba.default_handler     = {{ php_config_main_module_dba_default_handler }}


[opcache]
opcache.enable                  = {{ php_config_main_module_opcache_enable }}
opcache.enable_cli              = {{ php_config_main_module_opcache_enable_cli }}
opcache.memory_consumption      = {{ php_config_main_module_opcache_memory_consumption }}
opcache.interned_strings_buffer = {{ php_config_main_module_opcache_interned_strings_buffer }}
opcache.max_accelerated_files   = {{ php_config_main_module_opcache_max_accelerated_files }}
opcache.max_wasted_percentage   = {{ php_config_main_module_opcache_max_wasted_percentage }}
opcache.use_cwd                 = {{ php_config_main_module_opcache_use_cwd }}
opcache.validate_timestamps     = {{ php_config_main_module_opcache_validate_timestamps }}
opcache.revalidate_freq         = {{ php_config_main_module_opcache_revalidate_freq }}
opcache.revalidate_path         = {{ php_config_main_module_opcache_revalidate_path }}
opcache.save_comments           = {{ php_config_main_module_opcache_save_comments }}
opcache.load_comments           = {{ php_config_main_module_opcache_load_comments }}
opcache.fast_shutdown           = {{ php_config_main_module_opcache_fast_shutdown }}
opcache.enable_file_override    = {{ php_config_main_module_opcache_enable_file_override }}
opcache.optimization_level      = {{ php_config_main_module_opcache_optimization_level }}
opcache.inherited_hack          = {{ php_config_main_module_opcache_inherited_hack }}
opcache.dups_fix                = {{ php_config_main_module_opcache_dups_fix }}
opcache.blacklist_filename      = {{ php_config_main_module_opcache_blacklist_filename }}
opcache.max_file_size           = {{ php_config_main_module_opcache_max_file_size }}
opcache.consistency_checks      = {{ php_config_main_module_opcache_consistency_checks }}
opcache.force_restart_timeout   = {{ php_config_main_module_opcache_force_restart_timeout }}
opcache.error_log               = {{ php_config_main_module_opcache_error_log }}
opcache.log_verbosity_level     = {{ php_config_main_module_opcache_log_verbosity_level }}
opcache.preferred_memory_model  = {{ php_config_main_module_opcache_preferred_memory_model }}
opcache.protect_memory          = {{ php_config_main_module_opcache_protect_memory }}


[curl]
curl.cainfo = {{ php_config_main_module_curl_cainfo }}


